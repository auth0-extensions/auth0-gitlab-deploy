{
  "name": "auth0-gitlab-deploy-extension",
  "version": "2.0.1",
  "description": "Auth0 GitLab Deployment Extension",
  "engines": {
    "node": "5.9.0"
  },
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && npm run client:build && npm run extension:build && npm run extension:bundle",
    "clean": "rimraf dist",
    "client:build": "npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack/config.prod.js --colors -p",
    "extension:build": "cross-env NODE_ENV=production webpack -p --config ./build/extension/webpack.config.js",
    "extension:bundle": "cp ./dist/auth0-gitlab-deploy.extension.$npm_package_version.js ./build/bundle.js",
    "extension:size": "cross-env NODE_ENV=production webpack -p --config ./build/extension/webpack.config.js --json > ./build/extension/bundle-size.json && node ./build/extension/bundle-size.js",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern build --ignore-pattern webpack .",
    "serve:dev": "cross-env NODE_ENV=development nodemon --ignore assets/app/ --ignore build/webpack/ --ignore server/data.json --ignore client/ --ignore node_modules/ ./build/webpack/server.js",
    "serve:prod": "cross-env NODE_ENV=production node index.js",
    "tag": "git tag $npm_package_version && git push --tags",
    "test:clean": "rimraf ./coverage && rimraf ./.nyc_output",
    "test:pre": "npm run test:clean",
    "test:watch": "npm run test:pre && cross-env NODE_ENV=test mocha tests/mocha.js ./tests/**/*.tests.js --watch",
    "test": "npm run test:pre && cross-env NODE_ENV=test nyc mocha tests/mocha.js ./tests/**/*.tests.js",
    "win-build": "cross-env NODE_ENV=production && npm run clean && npm run client:build && npm run extension:build && npm run win-extension:bundle",
    "win-extension:bundle": "copy dist\\auth0-gitlab-deploy.extension.%npm_package_version%.js build\\bundle.js /y"
  },
  "keywords": [
    "auth0"
  ],
  "author": "Sandrino Di Mattia",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.2",
    "auth0": "^2.4.0",
    "auth0-extension-express-tools": "^0.9.0",
    "auth0-extension-tools": "^1.0.0",
    "auth0-oauth2-express": "^1.1.8",
    "auth0-source-control-extension-tools": "^2.0.0",
    "axios": "^0.15.0",
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "ejs": "^2.4.2",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "gitlab": "^1.7.0",
    "glob": "^7.0.5",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^7.1.9",
    "jwt-decode": "^2.0.1",
    "lodash": "^4.8.2",
    "lru-memoizer": "^1.6.0",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "nconf": "^0.8.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^4.1.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.72.0",
    "request-promise": "^3.0.0",
    "semver": "^5.1.0",
    "webtask-tools": "^3.0.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "cross-env": "^3.1.2",
    "css-loader": "^0.25.0",
    "eslint": "^3.3.1",
    "eslint-config-auth0": "^6.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^3.0.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.2",
    "nodemon": "^1.9.2",
    "nyc": "^8.3.1",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.13.0",
    "postcss-reporter": "^1.3.3",
    "postcss-simple-vars": "^3.0.0",
    "raw-loader": "^0.5.1",
    "react-bootstrap": "^0.30.5",
    "react-loader-advanced": "^1.4.0",
    "react-s-alert": "^1.1.4",
    "react-transform-hmr": "^1.0.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-form": "^5.2.5",
    "redux-simple-router": "^2.0.4",
    "redux-static": "^1.0.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-sources": "^0.1.2",
    "webpack-stats-plugin": "^0.1.1",
    "webtask-bundle": "^2.1.1"
  }
}
