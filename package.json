    "cross-env": "^3.1.3",
    "css-loader": "0.26.1",
    "eslint": "^3.9.1",
    "eslint-config-auth0": "^6.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "exports-loader": "^0.6.3",
