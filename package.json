    "gitlab": "3.4.2",
    "glob": "^7.0.5",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^7.1.9",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.8.2",
    "lru-memoizer": "^1.6.0",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "nconf": "^0.8.4",
    "qs": "^6.3.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "2.8.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^4.1.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.72.0",
    "request-promise": "^3.0.0",
